<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lenovots.crm.project.entity">
	<class name="EnvironmentVariable" table="t_environmentvariable">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="varName" column="var_name"></property>
		<property name="value" column="var_value"></property>
		<property name="dataType" column="data_type"></property>
	</class>
</hibernate-mapping>